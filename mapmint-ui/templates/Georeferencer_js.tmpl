function deleteGCP(com,grid){
    var hasValue=-1;
    \$('.trSelected td:first', grid).each(function() {
	hasValue=1;
	var index=parseInt(\$(this).text(), 10);
	try{
		points_layer.removeFeatures([points_layer.features[index]]);
		if(points_layer1.features[index])
			points_layer1.removeFeatures([points_layer1.features[index]]);
		for(i=0;i<points_layer.features.length;i++){
			points_layer.features[i].attributes.id=i;
			if(points_layer.features[i]){
				points_layer1.features[i].attributes.id=i;
			}
		}
		points_layer.redraw();
		points_layer1.redraw();
	}catch(e){alert(e);}
	loadCurrentpoints();
    });
    if(hasValue<0){
	if(!\$( "#dropgcpfile-window" )[0])
    		\$("body").append('<div id="dropgcpfile-window" title="$zoo._("Delete GCPs")"></div>');
    }
}

function loadCurrentpoints(){
  var prows=Array();
  for(i=0;i<points_layer.features.length;i++){
  for(j=0;j<points_layer.features.length;j++)
  if(points_layer.features[j].attributes.id==i){
    points_layer.features[j].fid=i;
    var m=points_layer.features[j].geometry.x;
    var n=points_layer.features[j].geometry.y;
    if(xx>yy){
      px=((m*xx)/(180/4));
      py=yy-((yy*n)/(((yy*180)/xx)/4));
    }else{
      px=(m*xx)/(((xx*90)/yy)/4);
      py=yy-((yy*n)/(90/4));
    }
    
    var hasElement=-1;
    for(k=0;k<points_layer1.features.length;k++){
    if(points_layer1.features[k].attributes.id==i){
      points_layer1.features[k].fid=points_layer.features[j].attributes.id;
      var d=points_layer1.features[k].geometry.clone();
      var d1=d.transform(new OpenLayers.Projection('EPSG:900913'),new OpenLayers.Projection('EPSG:4326'));
      var m1=d1.x;
      var n1=d1.y;
      prows.push({"id": i,"cell": [i,px,py,d1.x,d1.y]});
      hasElement=1;
      break;
    }
    }
    if(hasElement<0)
      prows.push({"id": i,"cell": [i,px,py,0,0]});
  }
  }
  \$('.flexiGCP').flexAddData({"rows":prows,"page":1,"total":prows.length});
  return prows;
}

