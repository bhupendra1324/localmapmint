DrawPoints = OpenLayers.Class(OpenLayers.Control.DrawFeature, {
    
    // this control is active by default
  autoActivate: false,
      
  initialize: function(layer, options) {
      // only points can be drawn
      var handler = OpenLayers.Handler.Point;
      OpenLayers.Control.DrawFeature.prototype.initialize.apply(
		this, [layer, handler, options]
      );
    },

  drawFeature: function(geometry) {
      OpenLayers.Control.DrawFeature.prototype.drawFeature.apply(
			 this, arguments	
      );
      this.deactivate();
      this.layer.redraw(true);
    }
  }
);


function startGCPEditor(){
      for(var i=0;i<points_layer.features.length;i++)
      	      points_layer.features[i].attributes["selected"]=0;
      points_layer.redraw();
      for(var i=0;i<points_layer1.features.length;i++)
      	      points_layer1.features[i].attributes["selected"]=0;
      points_layer1.redraw();
      var cnt=0;
      \$('.flexiGCP tr').each(function(){
	  if(\$(this).hasClass('trSelected')){
	      System.gcpID=cnt;
	      points_layer.features[cnt].attributes["selected"]=1;
	      points_layer.redraw();

	      if(!System.gcpEdit){
		  if(points_layer1.features.length<points_layer.features.length){
		    draw_points1.activate();
	      	  }else{
		    points_layer1.features[cnt].attributes["selected"]=1;
		    points_layer1.redraw();
		    drag_points.activate();
		    drag_points1.activate();
	      	  }
	      }else{
	              fillGCPEditor(cnt);
	      	      \$( "#gcp-editor-dialog" ).window({
	      	      	  minimizable:false,
	      		  maximizable:false,
	      		  resizable: false,
	      		  height:175,
	      		  width:400
	      	      });
	      }
	      cnt=-100;
	  }
	  else
	      cnt+=1;
      });
      if(cnt>=0){
	  draw_points1.deactivate();
	  drag_points.deactivate();
	  drag_points1.deactivate();
      }
  }

function loadGeorefProject(){
	\$.ajax({
	  type: "GET",
	  url: System.zooUrl+"?service=WPS&version=1.0.0&request=Execute&Identifier=vector-tools.mmExtractVectorInfo&DataInputs=dataSource=$conf["senv"]["mmGeoDST"];layer=$conf["senv"]["mmGeoDSO"]&RawDataOutput=Result",
	  dataType: "xml",
	  complete: function(xml,status) {
		var tmp=\$(xml.responseXML).find("size").text().split(',');
		xx=parseInt(tmp[0]);
		yy=parseInt(tmp[1]);

  var w,h;
  if(xx<yy){
    w=((xx*(90/4))/yy);
    h=90/4;
  }else{
    w=180/4;
    h=(yy*(180/4))/xx;
  }


    var options1 = {
    	controls: [
		  new OpenLayers.Control.Navigation(),
	     	  new OpenLayers.Control.ZoomBox({alwaysZoom:true})
	],
  	projection: new OpenLayers.Projection("EPSG:4326"),
  	displayProjection: new OpenLayers.Projection("EPSG:4326"),
	units: "m",
  	numZoomLevels: 18,
  	minZoomLevel: 1,
	maxExtent: new OpenLayers.Bounds(0, 0, 180/4, 90/4)
  };
  geoMap=new OpenLayers.Map('georefImg',options1);
   

		  var globe = new OpenLayers.Layer.Image(
        	      'GeoReferenceImage',
    		      #if $conf["main"]["tmpUrl"][:7]!="http://"#System.zooUrl+'/'+#end if#'$conf["main"]["tmpUrl"]/$conf["senv"]["mmGeoImg"]',
      		      new OpenLayers.Bounds(0, 0, w, h ),
    		      new OpenLayers.Size(xx/32,yy/32)
		  );
  		  geoMap.addLayer(globe);

  /**
   * Points layer for georeferencer map
   */
  points_layer = new OpenLayers.Layer.Vector("points");
    points_layer.styleMap=System.styleMap;
  points_layer.events.on({
    featureadded: function() {
      if(!points_layer.features[points_layer.features.length-1].attributes["id"] && points_layer.features[points_layer.features.length-1].attributes["id"]!=0)
        points_layer.features[points_layer.features.length-1].attributes["id"]=points_layer.features.length-1;
      loadCurrentpoints();
      \$('.flexiGCP').flexReload();
    }
  });
  geoMap.addLayer(points_layer);

  draw_points = new DrawPoints(points_layer);
  drag_points = new OpenLayers.Control.DragFeature(points_layer, {
      autoActivate: false,
      onStart: function(feature,pixel){
          if(feature.attributes.id==System.gcpID)
	    return true;
	  else{
	    drag_points.deactivate();
	    drag_points1.deactivate();
	    var cnt=0;
	    \$('.flexiGCP tr').each(function(){
	      if(\$(this).hasClass('trSelected')){
	        \$(this).removeClass('trSelected');
	      }
	      cnt++;
	    });
	    return false;
	  }
      }
	});
  drag_points.onComplete = function() {
    loadCurrentpoints();
    \$('.flexiGCP').flexReload();
  };
  geoMap.addControls([draw_points, drag_points]);
  geoMap.setCenter(geoMap.getCenter(),1);
  geoMap.zoomToMaxExtent();
  //geoMap.controls[i].activate();
  //geoMap.controls[i].activate();

	  }
	});
}

