function fillGCPEditor(cnt){
  for(var i=0;i<points_layer.features.length;i++)
  if(points_layer.features[i].attributes.id==cnt){
    points_layer.features[i].fid=i;
    var m=points_layer.features[i].geometry.x;
    var n=points_layer.features[i].geometry.y;
    if(xx>yy){
      px=((m*xx)/(180/4));
      py=yy-((yy*n)/(((yy*180)/xx)/4));
    }else{
      px=(m*xx)/(((xx*90)/yy)/4);
      py=yy-((yy*n)/(90/4));
    }
    if(points_layer1.features[i]){
      points_layer1.features[i].fid=i;
      var d=points_layer1.features[i].geometry.clone();
      var d1=d.transform(new OpenLayers.Projection('EPSG:900913'),new OpenLayers.Projection('EPSG:4326'));
      var m1=d1.x;
      var n1=d1.y;
      \$("#gcp_editor_imgx").val(px);
      \$("#gcp_editor_imgy").val(py);
      \$("#gcp_editor_mapx").val(d1.x);
      \$("#gcp_editor_mapy").val(d1.y);
    }
    else{
      \$("#gcp_editor_imgx").val(px);
      \$("#gcp_editor_imgy").val(py);
      \$("#gcp_editor_mapx").val(0);
      \$("#gcp_editor_mapy").val(0);
    }
  }
}

function saveGCPEditing(){
  pres=[[
    System.gcpID,
    parseFloat(\$("#gcp_editor_imgx").val()),
    parseFloat(\$("#gcp_editor_imgy").val()),
    parseFloat(\$("#gcp_editor_mapx").val()),
    parseFloat(\$("#gcp_editor_mapy").val())
  ]];
  var i=0;
  if(xx>yy){
    m=((180/4)*parseFloat(pres[i][1]))/xx;
    n=((yy-parseFloat(pres[i][2]))/yy)*(((yy*180)/xx)/4);
  }else{
    m=((parseFloat(pres[i][1]))/xx)*(((xx*90)/yy)/4);
    n=((yy-parseFloat(pres[i][2]))*(90/4)/yy);
  }

  gcp0=new OpenLayers.Geometry.Point(m,n);

  gcp=new OpenLayers.Geometry.Point(pres[i][3],pres[i][4]);
  wgs84=new OpenLayers.Projection('EPSG:4326');
  mercator=new OpenLayers.Projection('EPSG:900913');
  gcp.transform(wgs84,mercator);
  
  var hasElements=-1;
  for(k=0;k<points_layer.features.length;k++){
    if(points_layer.features[k].attributes.id==System.gcpID){
      points_layer.removeFeatures([points_layer.features[k]]);
      points_layer.addFeatures([new OpenLayers.Feature.Vector(gcp0,{id: System.gcpID})]);
      hasElements=1;
      break;
    }
  }

  hasElements=-1;
  for(j=0;j<points_layer1.features.length;j++){
    if(points_layer1.features[j].attributes.id==System.gcpID){
      points_layer1.removeFeatures([points_layer1.features[j]]);
      points_layer1.addFeatures([new OpenLayers.Feature.Vector(gcp,{id: System.gcpID})]);
      hasElements=1;
      break;
    }
  }
  if(hasElements<0){
    points_layer1.addFeatures([new OpenLayers.Feature.Vector(gcp,{id: System.gcpID})]);
  }

  if(points_layer.features.length>1)
    geoMap.zoomToExtent(points_layer.getDataExtent());
  if(points_layer1.features.length>1)
    map.zoomToExtent(points_layer1.getDataExtent());

  \$("#gcp-editor-dialog").window("close");
}

