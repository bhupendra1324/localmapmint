function loadGCPFile(){
  \$.ajax({
    type: "GET",
    url: System.zooUrl+"?service=WPS&version=1.0.0&request=Execute&Identifier=georeferencer.loadGCP&DataInputs=dso="+\$("#mapName").val()+";file="+\$("#csvfile").val()+"&RawDataOutput=Result",
    complete: function(xml,status) {
      if(checkWPSResult(xml,false)){
        points_layer.removeFeatures(points_layer.features);
        points_layer1.removeFeatures(points_layer1.features);
        var prows=new Array();
        var pres=eval(xml.responseText);
	wgs84=new OpenLayers.Projection('EPSG:4326');
	mercator=new OpenLayers.Projection('EPSG:900913');
	for(var i=0;i<pres.length;i++){
      	    prows.push({"id": i,"cell": pres[i]});
    	    if(xx>yy){
      	      m=((180/4)*parseFloat(pres[i][1]))/xx;
      	      n=((yy-parseFloat(pres[i][2]))/yy)*(((yy*180)/xx)/4);
    	    }else{
      	      m=((parseFloat(pres[i][1]))/xx)*(((xx*90)/yy)/4);
      	      n=((yy-parseFloat(pres[i][2]))*(90/4)/yy);
    	    }
	    gcp0=new OpenLayers.Geometry.Point(m,n);
	    gcp=new OpenLayers.Geometry.Point(parseFloat(pres[i][3]),parseFloat(pres[i][4]));
	    gcp.transform(wgs84,mercator);
	    points_layer.addFeatures([new OpenLayers.Feature.Vector(gcp0,{id: i,selected: 0})]);
	    points_layer1.addFeatures([new OpenLayers.Feature.Vector(gcp,{id: i,selected: 0})]);
	}
	geoMap.zoomToExtent(points_layer.getDataExtent());
	map.zoomToExtent(points_layer1.getDataExtent());
        \$('.flexiGCP').flexAddData({"rows":prows,"page":1,"total":prows.length});
	\$( "#loadgcp-window" ).window("close");
      }
    }
  });

}

